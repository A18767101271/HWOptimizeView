<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wislie.hwoptimizelayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.wislie.customview.MyApp"
        android:theme="@style/AppTheme">
        <activity android:name="com.wislie.customview.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 仿华为自定义view-->
        <activity android:name="com.wislie.customview.huawei.activity.HwActivity"/>
        <!-- 仿vivo自定义view-->
        <activity android:name="com.wislie.customview.vivo.activity.VivoActivity"/>
    </application>

</manifest>